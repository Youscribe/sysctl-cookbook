# -*- conf -*-
# Generated by chef -- local modifications will be lost!
#
# This file contains settings from node['sysctl']
<% if node.attribute?('sysctl') and not node['sysctl'].nil? -%>
<%   node['sysctl'].keys.sort.each do |key| -%>

# <%= key %>
<%     cfg = node['sysctl'][key] -%>
<%     cfg.keys.sort.each do |var| -%>
<%= var %> = <%= cfg[var] %>
<%     end -%>
<%   end -%>
<% end -%>
